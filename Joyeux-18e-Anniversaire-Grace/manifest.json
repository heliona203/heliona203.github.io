{
  "name": "Joyeux 18e Anniversaire Grace",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icons8-minecraft-grass-cube-16.png",
      "32": "/images/icons8-minecraft-grass-cube-32.png",
      "48": "/images/icons8-minecraft-grass-cube-48.png",
      "128": "/images/icons8-minecraft-grass-cube-128.png"
    }
  },
  "icons": {
    "16": "/images/icons8-minecraft-grass-cube-16.png",
    "32": "/images/icons8-minecraft-grass-cube-32.png",
    "48": "/images/icons8-minecraft-grass-cube-48.png",
    "128": "/images/icons8-minecraft-grass-cube-128.png"
  },
  "options_page": "options.html"
}
